    "templateHtml": "<div class=\"main-widget\">\r\n    <div class=\"header\">\r\n        <div>\r\n            <h3 class=\"title\">{{title}}</h3>\r\n            <p class=\"subtitle\">{{subtitle}}</p>\r\n        </div>\r\n        <div class=\"form-check form-switch switch\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" role=\"switch\" id=\"flexSwitchCheckDefault\"\r\n                [checked]=\"checkState\" (change)=\"gpioToggleChange($event)\">\r\n        </div>\r\n    </div>\r\n    <div class=\"iframe-wrapper\">\r\n        <iframe *ngIf=\"checkState\" class=\"responsive-iframe\" src=\"https://rtsp.me/embed/faN3T75N/?\"\r\n            frameborder=\"0\" allowfullscreen></iframe>\r\n        <div class=\"hidden\" *ngIf=\"!checkState\">\r\n            <i class=\"bi bi-eye-slash-fill icon\"></i>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
